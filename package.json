{
  "name": "amexio-ng-extensions",
  "version": "5.80.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run postbuild",
    "build:watch": "npm run build && ng build --watch &&  npm run postbuild",
    "build:prod": "ng build --prod  && npm run postbuild",
    "postbuild": "npm run copystyles",
    "copystyles": "cp -r projects/amexio-ng-extensions/src/lib/styles dist/amexio-ng-extensions/",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "check-travis": "travis-status --repo=meta-magic/amexio.github.io/ --branch=v5.80.1 --fail-pending || echo 'Not yet passing'",
    "sonar-scanner": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.803.19",
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "angular-font-awesome": "3.1.2",
    "font-awesome": "4.7.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "tslint-sonarts": "1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-opera-launcher": "^1.0.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
