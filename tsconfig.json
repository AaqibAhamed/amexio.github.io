{
  "compilerOptions": {
    "downlevelIteration": true,
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "inlineSources": false,
    "declaration": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}